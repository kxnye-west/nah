# Server Port
PORT=8080

# Authentication Configuration
# Set to "true" to enable basic authentication
AUTH_ENABLED=false

# Users configuration (JSON string format)
# Only used if AUTH_ENABLED is "true"
# Example: AUTH_USERS={"admin":"password123","user":"pass456"}
AUTH_USERS={}

# CORS Configuration
# Set the allowed origin for CORS requests
# Use your domain or the appropriate origin
# Default: http://localhost:8080
CORS_ORIGIN=http://localhost:8080

# License Key (if using Masqr)
# This is optional and used by the Masqr license validation system
MASQR=false
